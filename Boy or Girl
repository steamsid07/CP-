// Boy or Girl

#include <bits/stdc++.h>

using namespace std;

int main()

{

    string s;
    cin >> s;

    unordered_set<char> sa;
    int flag = 0;

    for (int i = 0; i < s.size(); i++)
    {
        sa.insert(s[i]);
        }

    if (sa.size() % 2 == 0)
    {
        cout << "CHAT WITH HER!" << endl;
    }
    else
    {
        cout << "IGNORE HIM!" << endl;
    }

    return 0;
}
