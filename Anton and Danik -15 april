// Anton and Danik

#include <bits/stdc++.h>

using namespace std;
void solve()
{

    int n, dan = 0, ant = 0;
    cin >> n;

    string s;
    cin >> s;

    for (int i = 0; i < s.length(); i++)
    {

        if (s[i] == 'D')
        {
            dan++;
        }

        else
        {
            ant++;
        }
    }

    if (ant < dan)
    {
        cout << "Danik"
             << "\n";
    }
    else if (dan < ant)
    {
        cout << "Anton"
             << "\n";
    }

    else if (dan == ant)
    {
        cout << "Friendship"
             << "\n";
    }
}

int main()
{

    ios_base::sync_with_stdio(false);
    cin.tie(0);

    solve();
    return 0;
}
